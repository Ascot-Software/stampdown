{
  "name": "@stampdwn/cli",
  "version": "0.1.0",
  "description": "Command-line interface for Stampdown",
  "main": "dist/cli.js",
  "types": "dist/cli.d.ts",
  "bin": {
    "stampdown": "./bin/stampdown.js"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish": "npm publish --access public"
  },
  "keywords": [
    "markdown",
    "template",
    "cli",
    "stampdown"
  ],
  "author": "Devon Anderson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ascot-Software/stampdown.git",
    "directory": "packages/cli"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@stampdwn/core": "^0.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0"
  }
}
