{
  "comments": {
    "lineComment": "//",
    "blockComment": ["{{!--", "--}}"]
  },
  "brackets": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["{{", "}}"],
    ["{{>", "}}"],
    ["{{#>", "}}"], //failover partial - opening
    ["{{#*", "}}"],
    ["{{{{", "}}}}"],
    ["{{{{/", "}}}}"]
  ],
  "autoClosingPairs": [
    { "open": "{", "close": "}" },
    { "open": "[", "close": "]" },
    { "open": "(", "close": ")" },
    { "open": "{{", "close": "}}" },
    { "open": "{{#", "close": "{{/" },
    { "open": "\"", "close": "\"" },
    { "open": "'", "close": "'" },
    { "open": "`", "close": "`" },
    { "open": "**", "close": "**" },
    { "open": "*", "close": "*" },
    { "open": "_", "close": "_" }
  ],
  "surroundingPairs": [
    ["{", "}"],
    ["[", "]"],
    ["(", ")"],
    ["{{", "}}"],
    ["\"", "\""],
    ["'", "'"],
    ["`", "`"],
    ["**", "**"],
    ["*", "*"],
    ["_", "_"]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*{{#",
      "end": "^\\s*{{/"
    }
  }
}
