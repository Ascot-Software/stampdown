{
  "name": "@stampdwn/core",
  "version": "0.1.1",
  "description": "Stampdown is a Markdown templating language, that is simple and extensible.",
  "main": "dist/index.js",
  "types": "dist/core.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "publish": "npm publish --access public",
    "api": "api-extractor run",
    "docs": "api-documenter markdown --input-folder ./temp --output-folder ./docs",
    "ai-docs": "mkdir -p ../../.github/api/core/ && cp ./docs/**.md ../../.github/api/core/"
  },
  "keywords": [
    "markdown",
    "template",
    "templating",
    "handlebars",
    "expressions"
  ],
  "author": "Devon Anderson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ascot-Software/stampdown.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0"
  }
}
