{
  "name": "@stampdwn/llm",
  "version": "0.1.1",
  "description": "LLM prompt engineering plugin for Stampdown",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "publish": "npm publish --access public",
    "api": "api-extractor run",
    "docs": "api-documenter markdown --input-folder ./temp --output-folder ./docs",
    "ai-docs": "mkdir -p ../../.github/api/llm/ && cp ./docs/**.md ../../.github/api/llm/"
  },
  "keywords": [
    "llm",
    "ai",
    "prompt",
    "templating",
    "stampdown"
  ],
  "author": "Devon Anderson",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ascot-Software/stampdown.git",
    "directory": "packages/llm"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "@stampdwn/core": "^0.1.0",
    "@toon-format/toon": "^0.7.3",
    "gpt-tokenizer": "^3.0.1",
    "yaml": "^2.8.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.37.0"
  }
}
