{
  "name": "stampdown",
  "version": "0.1.0",
  "description": "A Markdown templating language with Handlebars-like features",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "sdt-cli": "./bin/sdt-cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "extension:package": "cd vscode-extension && npx @vscode/vsce package",
    "extension:publish": "cd vscode-extension && npx @vscode/vsce publish"
  },
  "keywords": [
    "markdown",
    "template",
    "templating",
    "handlebars",
    "expressions"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "ts-jest": "^29.1.5",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@ai-sdk/provider": "^2.0.0",
    "gpt-tokenizer": "^3.0.1",
    "marked": "^12.0.2",
    "yaml": "^2.8.1",
    "zod": "^4.1.11"
  }
}
